<!-- Global site tag (gtag.js) - Google Analytics
        Here's hoping this shit don't get hijacked lmaooooo -->
<script async src="https://www.googletagmanager.com/gtag/js?id=UA-217946576-1"></script>
<script>
    window.dataLayer = window.dataLayer || [];
    function gtag() { dataLayer.push(arguments); }
    gtag('js', new Date());

    gtag('config', 'UA-217946576-1');
</script>

